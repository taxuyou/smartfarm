{
    "_comment": "XGBoost Decision Tree for 3rd Data",
    
    "predict": "rda_tomato_infer",
    
    "data":{
        "path": "./dataset/3rd_data",
        "raw_data":{
            "names": ["농진청-29농가-환경생육생산데이터.xlsx"]
        },
        "preprocessed_data":{
            "names": ["농진청-29농가-환경생육생산데이터-dt-dataframe.csv"]
        },
        "supervised_data":{
            "names": ["농진청-29농가-환경생육생산데이터-dt-supervised-dataframe.csv"],
            "lookback": 8,
            "out": 1
        }
    },

    "model":{
        "name":"decision_tree",
        "pretrained": true,
        "config": {
            "learning_rate": 0.0001
        }
    },
 
    "train":{
        "test_samples": 12
    },

    "infer":{
        "test_samples": 10,
        "metric": "mse"
    },

    "util":{
        "_comment": "Path will be assigned, if needed."
    },

    "device":{
        "_comment": "Single GPU is only concerned."
    }
}

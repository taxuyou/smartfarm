        "growth_heatmap": {
            "avail": true,
            "name": "growth_heatmap.png",
            "x_labels": ["sample_number", "weekly_growth_length(cm)", "plant_length(cm)", "stem_thickness(mm)", "leaf_length(cm)", "leaf_width(cm)",

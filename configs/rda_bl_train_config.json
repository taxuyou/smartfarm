{
    "_comment": "Keras Long-Short Term Memory for 3rd Data",
    
    "predict": "rda_tomato_train",
    
    "data":{
        "path": "./dataset/3rd_data",
        "raw_data":{
            "names": ["농진청-29농가-환경생육생산데이터.xlsx"]
        },
        "preprocessed_data":{
            "names": ["농진청-29농가-환경생육생산데이터-bl-dataframe.csv"]
        },
        "supervised_data":{
            "names": ["농진청-29농가-환경생육생산데이터-bl-supervised-dataframe.csv"],
            "lookback": 8,
            "out": 1
        }
    },

    "model":{
        "name":"basic_lstm",
        "pretrained": false,
        "config": {
            "model_path": ".",
            "input_shapes": [],
            "output_shape": []
        }
    },

    "train":{
        "test_samples": 12,
        "epochs": 500,
        "batch": 512,
        "metric": "mse"
    },

    "optimizer":{
        "algorithm": "adam",
        "config": {
            "learning_rate": 0.0001
        }
    },

    "util":{
        "_comment": "Path will be assigned, if needed."
    },

    "device":{
        "name": "/device:GPU:0"
    }
}

{
    "predict": "mlp_train",
    "target": "growth",
    
    "data":{
        "train_data":{
            "path": "dataset/2nd_data/myeong/",
            "growth" : "myeong_growth_detail_data.xlsx",
            "env" : "myeong_env_detail_data.xlsx",
            "product_1" : "myeong_product_1_data.xlsx",
            "product_2" : "myeong_product_2_data.xlsx",
            "product_3" : "myeong_product_3_data.xlsx",
            "product_4" : "myeong_product_4_data.xlsx"
        },
        "label_data":{
            "path": "./jypark/2nd_data",
            "names": ["harvest_label.csv", "farm_data_label.csv"]
        },
        "num_samples": 16
    },

    "model":{
        "name":"mlp",
        "pretrained": false,
        "config": {
            "permute": false,
            "avg": true,
            "model_path": ".",
            "input_shapes": [],
            "output_shape": [],
            "explain": true
        }
    },

    "train":{
        "epochs": 5000,
        "batch": 16,
        "metric": "mse"
    },

    "optimizer":{
        "algorithm": "sgd",
        "config": {
            "learning_rate": 0.001,
            "momentum": 0.9,
            "nesterov": true
        }
    },

    "util":{
        "path": "./experiments/mlp",
        "model": "/mlp_env2grw",
        "first_model": "/mlp_env2grw",
        "second_model": "/mlp_grw2pro",
        "train_stats":"/train_stats.csv",
        "bar": {
            "avail": true,
            "name": "env_bar.png"
        }
    },

    "device":{
        "name": "/device:GPU:0"
    }
}